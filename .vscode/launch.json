        },
        {
            "name": "MULTI-NEEDLE: Debug openai single call (gpt-4o)",
            "type": "debugpy",
            "request": "launch",
            "module": "needlehaystack.run",
            "args": [
                "--provider=openai",
                "--model_name=gpt-4o",
                "--config=.vscode/multi-config.json"
            ],
        },
        {
            "name": "MULTI-NEEDLE: Debug awareness single call (gpt-4-medium)",
            "type": "debugpy",
            "request": "launch",
            "module": "needlehaystack.run",
            "args": [
                "--provider=awareness",
                "--model_name=gpt-4-medium",
                "--config=.vscode/multi-config.json"
